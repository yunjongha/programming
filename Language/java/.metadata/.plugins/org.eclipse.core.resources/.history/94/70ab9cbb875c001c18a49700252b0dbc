package chapter1_lamda;

public class VarScope {

	/**
	 * 
	 * @param args
	 */
	public static void main(String[] args) {
		
	}
	
	/**
	 * 
	 * @param text
	 * @param count
	 */
	public static void repeatMessage(String text, int count) {
		Runnable r = () -> {
			for (int i = 0 ; i < count; i++) {
				System.out.println(ext);
			}
		}
	}
}
