package chapter2_stream;

public class CollectorMap {
	
	private static Person[] persons = new Person[] { new Person(10, "Bin"), new Person(15, "Mu"), new Person(40, "Ha"), new Person(40, "Hyeon") };
	
	public static void main(String[] args) {
		
	}
	
	

}

class Person {

	private int id;
	private String name;
	
	public Person(int id, String name) {
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}
	
	
	public String getName() {
		return name;
	}
}
